{
  "expo": {
    "name": "ziplan",
    "slug": "ziplan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ziplanlogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ziplanlogo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "manifest": {
        "name": "ziplan",
        "short_name": "ziplan",
        "display": "standalone",
        "start_url": ".",
        "icons": [
          {
            "src": "./assets/images/ziplanlogo.png",
            "sizes": "512x512",
            "type": "image/png"
          },
          {
            "src": "./assets/images/ziplanlogo.png",
            "sizes": "192x192",
            "type": "image/png"
          }
        ]
      }
    }
  }
}
